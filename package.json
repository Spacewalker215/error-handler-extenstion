{
  "name": "error-handler-extension",
  "displayName": "Error Handler Extension",
  "description": "Interprets console errors and provides friendly explanations using ChatGPT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.handleError"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.handleError",
        "title": "Handle Error with ChatGPT"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "openai": "^4.61.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
